{
  "name": "ts-node-express",
  "version": "1.0.0",
  "description": "",
  "author": "Tomas Madariaga - tomasmax",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only dist/server.js",
    "dev": "NODE_ENV=development nodemon",
    "test": "jest --verbose --runInBand",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --no-cache --runInBand",
    "test:watch": "jest --watchAll --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --collectCoverage",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "license": "ISC",
  "homepage": "",
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
